<!DOCTYPE html>
<html>
<head>
  <style>
    #debug{
        background-color: #b0b09a;
    }
  </style>

  <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>

  <title>Facebook Plugin Test</title>

  <meta content="http://weeds.herokuapp.com" property="og:href">

  <script>
      var fb_id = '593946380691883';
      var fb_div = '<span><div id="fb_like" class="fb-like" data-href="https://www.facebook.com/NorthwestMassageandHealingCenter" data-layout="button" data-action="like" data-show-faces="false" data-share="true"></div></span>';


      window.fbAsyncInit = function() {
          FB.init({
              appId: fb_id,
              status: false,
              xfbml: true
          });
//
//          FB.api('/me', function(r){
//              console.log(r);
//          });
      };

      (function(d, s, id){
          var js, fjs = d.getElementsByTagName(s)[0];
          if (d.getElementById(id)) {return;}
          js = d.createElement(s); js.id = id;
//          js.src = "//connect.facebook.net/en_US/all.js";
          js.src = "//connect.facebook.net/en_US/all/debug.js";
          fjs.parentNode.insertBefore(js, fjs);
      }(document, 'script', 'facebook-jssdk'));

      window.onload = function(){
//          var debug = document.getElementById('debug');
//          var fb_like = document.getElementById('fb-like');
//          insertAfter(debug, fb_like);


          $('#sn_general').insertAfter('#debug');
      }

      function insertAfter(referenceNode, newNode) {
          referenceNode.parentNode.insertBefore(newNode, referenceNode.nextSibling);
      }

      function likeButton(parent_selector){
          var url = document.URL;
          var html = '<div id="sn_specific"><fb:like href="' + url +
                  '" layout="button_count" show_faces="false" width="65" action="like" colorscheme="light" ' +
                  'data-share="true" /></div>';
          $(parent_selector).append(html);
          if(typeof FB !== 'undefined'){
              FB.XFBML.parse(document.getElementById('sn_specific'));
          }
      }

      $(document).ready(function(){
          likeButton('#debug');
      });
  </script>
</head>

<body>
  <div id="fb-root"></div>

  <div id="sn_general">
  <span>
    <div class="fb-like" data-href="https://www.facebook.com/NorthwestMassageandHealingCenter"
         data-layout="button" data-action="like" data-show-faces="false" data-share="false"></div>
    </span>
    <span>
      us on Facebook
    </span>
  </div>

  <div id="sn_specific">
    <span>
    <!--<div class="fb-like" data-href="http://weeds.herokuapps.com"-->
         <!--data-layout="button_count" data-action="like" data-show-faces="false" data-share="true"></div>-->

    </span>
    </div>



  <div id="debug">
